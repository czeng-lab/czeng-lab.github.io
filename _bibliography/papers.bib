	pages = {1690--1699},
	file = {Noel et al. - 2011 - Quantification of Primary Amine Groups Available f.pdf:C\:\\Users\\曾诚\\Zotero\\storage\\BXXGLR3I\\Noel et al. - 2011 - Quantification of Primary Amine Groups Available f.pdf:application/pdf},
