	file = {Zeng et al. - 2021 - Virus Mechanics under Molecular Crowding.pdf:C\:\\Users\\曾诚\\Zotero\\storage\\JLDK38PH\\Zeng et al. - 2021 - Virus Mechanics under Molecular Crowding.pdf:application/pdf},
}

